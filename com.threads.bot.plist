<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.threads.bot</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/usr/bin/python3</string>
        <string>/Users/admin/Threads/bot.py</string>
    </array>
    
    <key>WorkingDirectory</key>
    <string>/Users/admin/Threads</string>
    
    <key>StandardOutPath</key>
    <string>/Users/admin/Threads/threads_bot.log</string>
    
    <key>StandardErrorPath</key>
    <string>/Users/admin/Threads/threads_bot.log</string>
    
    <key>RunAtLoad</key>
    <true/>
    
    <key>KeepAlive</key>
    <dict>
        <key>SuccessfulExit</key>
        <false/>
        <key>Crashed</key>
        <true/>
    </dict>
    
    <key>ThrottleInterval</key>
    <integer>10</integer>
</dict>
</plist>

